CREATE TABLE `periodicaledition`.`news` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `title` VARCHAR(45) NOT NULL,
  `body` VARCHAR(255) NOT NULL,
  `author` VARCHAR(45) NOT NULL,
  `camelcase` INT NOT NULL,
  PRIMARY KEY (`id`),
  INDEX `idcamelcase_idx` (`camelcase` ASC),
  CONSTRAINT `idcamelcase`
    FOREIGN KEY (`camelcase`)
    REFERENCES `periodicaledition`.`camelcase` (`id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);
